<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.server.model.Session" table="Session">

		<id column="SESSIONID" name="sessionId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="AccessToken" name="accessToken" type="java.lang.String" />
		<property column="PushId" name="pushId" type="java.lang.String" />
		<component name="location" class="com.application.server.model.pojo.Location">
			<property name="latitude" column="LATITUDE" type="java.lang.Double" />
			<property name="longitude" column="LONGITUDE" type="java.lang.Double" />
		</component>
		<property column="UserId" name="userId" type="java.lang.Integer" />
		<property column="Created" name="created" type="java.lang.Long" />
		<property column="Modified" name="modified" type="java.lang.Long" />
		<property name="deviceId" type="java.lang.String" >
			<column name="DeviceId" not-null="true"></column>
		</property>

	</class>
</hibernate-mapping>
