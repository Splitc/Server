<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.server.model.User" table="User">

		<id column="UserRideId" name="userRideId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property column="RideId" name="rideId" type="java.lang.Integer" />
		<property column="UserId" name="userId" type="java.lang.Integer" />
		<property column="TravellerId" name="travellerId" type="java.lang.Integer" />
		<property column="Rating" name="rating" type="java.lang.Double" />
		<property column="Created" name="created" type="java.lang.Longouble" />
		
	</class>
</hibernate-mapping>
 